    <script>
        function MultiSelector() {
            var selectorId;
            var area;
            var input;
            var selected = [];
            var maxToSelect = 2;

            this.init = function (id) {
                selectorId = id;
                area = $('#' + selectorId);
                input = area.find('.multi-selector-input');
                //                input.click(show);
                $(document.body).click(showHide);
                area.find('input[type=checkbox]').click(setInput);
                setInput();
                return this;
            };
            this.getData = function () {
                return selected;
            };

            var setInput = function () {
                input.empty();

                area.find('input[type=checkbox]').each(function (index, item) {
                    if (item.checked) {
                        for (var sItem of selected) {
                            if (item.value === sItem.value) {
                                return;
                            }
                        }
                        if (selected.length < maxToSelect) {
                            selected.push({
                                name: item.dataset.label,
                                value: item.value,
                            });
                        } else {
                            item.checked = false;
                            return;
                        }
                    } else {
                        for (var i = selected.length - 1; i >= 0; i--) {
                            sItem = selected[i];
                            if (item.value === sItem.value) {
                                selected.splice(i, 1);
                                return;
                            }
                        }
                    }

                });

                for (var item of selected) {
                    var newElem = $('<div class="selected-item">' + item.name + ' <i class="fa fa-times close-icon" aria-hidden="true" data-value="' + item.value + '"></i></div>');
                    newElem.find('.close-icon').click(removeItem);
                    input.append(newElem);
                }

                if (!selected.length) {
                    var text = input.attr('placeholder');
                    input.append($('<div class="helper">' + text + '</div>'));
                }
            };

            var removeItem = function (e) {
                var current = e.target;
                area.find('input[type=checkbox]').each(function (index, item) {
                    if (current.dataset.value == item.value) {
                        item.checked = false;
                    }
                });

                setInput();
            };

            var showHide = function (e) {
                var current = e.target;

                if ($(current).hasClass('close-icon') || $(current).hasClass('selected-item')) {
                    return;
                }

                var isFound = false;
                while (true) {
                    if (!current || current == document.body) {
                        break;
                    }
                    if (current.id == selectorId) {
                        isFound = true;
                        break;
                    }
                    current = current.parentNode;
                }
                if (isFound) {
                    area.find('.select-options').show();
                } else {
                    area.find('.select-options').hide();
                }

            }
        }

        function AirQualitiQuiz() {
            var currentPage = 0;
            var timeranges;
            var data = {
                address_id: null,
                smell_type: null,
                date: null,
                time_ranges: null,
                frequency: null,
                comment: null
            }

            var getPageId = function (index) {
                return 'quiz-page-id-' + index;
            };

            var showPage = function (index, isShow) {
                var page = document.getElementById(getPageId(index));
                if (page) {
                    page.style.display = isShow ? '' : 'none';
                    if (isShow) {
                        currentPage = index;
                    }
                }
            };

            var validatePage = function () {
                switch (currentPage) {
                    case 0:
                        var address = document.getElementById('address-autocomplete');
                        $('#address-data').removeClass('error');
                        if (address.dataset.selectedId) {
                            data.address_id = address.dataset.selectedId;
                            return true;
                        }
                        $('#address-data').addClass('error');
                        return false;
                    case 1:
                        $('#smell-type-data').removeClass('error');
                        var smellType = null;
                        $("input[name='smell_type']").each(function (index, item) {
                            if (item.checked) {
                                smellType = item.value;
                            }
                        });

                        if ('Другое' == smellType) {
                            smellType = $("input[name='smell_type_custom']").val();
                        }
                        if (smellType && smellType.trim()) {
                            data.smell_type = smellType.trim();
                            return true;
                        }
                        $('#smell-type-data').addClass('error');
                        return false;
                    case 2:
                        $('#date-data').removeClass('error');
                        $('#time-data').removeClass('error');
                        var isPassed = true;
                        var date = document.getElementById('report-date');
                        if (!date.value) {
                            isPassed = false;
                            $('#date-data').addClass('error');
                        } else {
                            data.date = date.value;
                        }
                        if (!timeranges.getData().length) {
                            isPassed = false;
                            $('#time-data').addClass('error');
                        } else {
                            data.time_ranges = timeranges.getData();
                        }
                        return isPassed;
                    case 3:
                        $('#frequency-data').removeClass('error');
                        var frequency = null;
                        $("input[name='frequency']").each(function (index, item) {
                            if (item.checked) {
                                frequency = item.value;
                            }
                        });

                        if (frequency) {
                            data.frequency = frequency;
                            return true;
                        }
                        $('#frequency-data').addClass('error');
                        return false;
                    case 4:
                        data.comment = document.getElementById('comment').value;
                        return true;

                }
                return true;
            };
            this.nextPage = function () {
                if (validatePage()) {
                    showPage(currentPage, false);
                    showPage(currentPage + 1, true);
                    if(currentPage === 5){
                        sendData();
                    }
                }
            };

            var sendData = function(){
                var id = getId();
                $.post(
                    '/wp-content/themes/moseco/quiz.php?type=submit', 
                    {data: JSON.stringify(data), id: id},
                    function(response){
                        setId(response.id);
                    }
                );
            };
            
            var getId = function(){
                return localStorage.getItem('air_quiz_uid');
            };
            
            var setId = function(id){
                localStorage.setItem('air_quiz_uid', id);
            };

            var updateSmellTypeCusom = function(e){
                var current = e.target;
                if(current.value === 'Другое'){
                    $("input[name='smell_type_custom']").prop( "disabled", false );
                }else {
                    $("input[name='smell_type_custom']").prop( "disabled", true );
                    $("input[name='smell_type_custom']").val('');
                }
            }
            this.init = function () {
                for (var i = 0; i < 7; i++) {
                    showPage(i, i == currentPage ? true : false);
                }

                jQuery(function () {
                    
                    $('input[type=radio][name=smell_type]').change(updateSmellTypeCusom);

                    timeranges = (new MultiSelector()).init('report-time-ranges');

                    $("#address-autocomplete").autocomplete({
                        source: '/wp-content/themes/moseco/quiz.php?type=address-lookup',
                        appendTo: "#address-autocomplete-options",
                        open: function (event, ui) {
                            $('#address-autocomplete-options').show();
                        },
                        close: function (event, ui) {
                            $('#address-autocomplete-options').hide();
                        },
                        change: function (event, ui) {
                            if (ui.item) {
                                this.dataset.selectedId = ui.item.id;
                            } else {
                                this.dataset.selectedId = '';
                            }
                        }
                    });

                    jQuery(".datepicker").datepicker({
                        nextText: "&#10095;",
                        prevText: "&#10094;",
                        maxDate: new Date(),
                        classes: {
                            'ui-widget': 'test'
                        },
                        dateFormat: "dd.mm.yy",
                        regional: "ru",
                        beforeShow: function (input, inst) {
                            $('#ui-datepicker-div').addClass("datepicker-widget");
                            if (false) {
                                $('#ui-datepicker-div').addClass("light-theme");
                            }
                            setTimeout(function () {
                                $('#ui-datepicker-div').outerWidth($(input).outerWidth())
                            }, 5);

                            if (this.classList.contains('expand-up')) {
                                setTimeout(function () {
                                    inst.dpDiv.css({
                                        top: $(".datepicker").offset().top - $('#ui-datepicker-div').outerWidth() + 41,
                                    });
                                }, 10);
                            }
                        },
                    });
                });


                return this;
            };
        }
        var quiz = (new AirQualitiQuiz()).init();

    </script>

                    
              












